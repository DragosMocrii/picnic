{
    "name": "mjolnic/drumbs",
    "description": "On-demand image manipulation tool for creating thumbs for your project assets on the fly",
    "keywords": ["image", "thumbnail", "thumb", "gd", "wideimage", "assets"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://github.com/mjolnic"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "smottt/wideimage": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mjolnic/WideImage.git"
        }
    ],
    "autoload": {
        "psr-0": {"\\Mjolnic\\Drumbs": "drumbs/src"}
    },
    "config": {
        "vendor-dir": "vendor"
    }
}