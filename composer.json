{
    "name": "thor/picnice",
    "description": "Laravel 4 On Demand Image Post-processing package (Image manipulation on the fly).",
    "keywords": ["thumbnail", "thumb", "wideimage", "assets", "laravel", "thorfw", "thor"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://www.mjolnic.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "illuminate/support": "4.1.*",
        "smottt/wideimage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Thor\\Picnice\\": "src/Picnice"
        }
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
